<!--部分テンプレート-->
<%= render 'books/user_info', book: @user_info %>

<h2>Users</h2>

<table>
  <tr>
    <th>image</th>
    <th>name</th>
    <th></th>
  </tr>

<% @users.each do |user| %>

  <tr>
    <td><%= image_tag user.get_profile_image(80, 80) %></td>
    <td><%= user.name %></td>
    <td><%= link_to "show", user_path(user.id) %></td>
  </tr>
  <% end %>
</table>